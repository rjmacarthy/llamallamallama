{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Launch Server",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": ["main:app","--reload"],
      "cwd": "${workspaceFolder}/backend",
    },
  ]
}